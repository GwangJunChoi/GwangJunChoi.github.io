{
  // workspace
  "debug.javascript.autoAttachFilter": "onlyWithFlag",
  "diffEditor.codeLens": true,
  "breadcrumbs.filePath": "off",
  "breadcrumbs.showArrays": false,
  "breadcrumbs.showConstants": false,
  "breadcrumbs.showEnumMembers": false,
  "breadcrumbs.showFields": false,
  "breadcrumbs.showKeys": false,
  "breadcrumbs.showNull": false,
  "breadcrumbs.showNumbers": false,
  "breadcrumbs.showPackages": false,
  "breadcrumbs.showOperators": false,
  "breadcrumbs.showStrings": false,
  "breadcrumbs.showVariables": false,
  "breadcrumbs.showBooleans": false,
  "breadcrumbs.showProperties": false,
  "breadcrumbs.showEvents": false,
  "breadcrumbs.showFiles": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": false,
  "editor.wordWrapColumn": 150,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.detectIndentation": false,
  "editor.fontFamily": "'Droid Sans Mono', 'Consolas', monospace",
  "editor.fontSize": 12,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.overviewRulerBorder": false,
  "editor.quickSuggestionsDelay": 200,
  "editor.linkedEditing": true,
  "editor.suggestSelection": "first",
  "editor.suggest.showReferences": false,
  "editor.suggest.showUsers": false,
  "editor.suggest.showValues": false,
  "editor.suggest.showWords": false,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": false,
  "editor.wordBasedSuggestions": false,
  "editor.wrappingIndent": "deepIndent",
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "explorer.autoReveal": false,
  "explorer.copyRelativePathSeparator": "/",
  "extensions.ignoreRecommendations": false,
  "files.autoSaveDelay": 600000,
  "files.associations": {
    "*.html": "php",
    "*.inc": "php"
  },
  "files.watcherExclude": {
    "**/bin/**": true,
    "**/build/**": true,
    "**/dist/**": true,
  },
  "git.autofetch": true,
  "git.suggestSmartCommit": false,
  "git.terminalGitEditor": true,
  "gitlens.advanced.messages": {
    "suppressGitDisabledWarning": true
  },
  "intelephense.environment.phpVersion": "5.6.33",
  "intelephense.format.braces": "k&r",
  "less.lint.importStatement": "error",
  "less.lint.propertyIgnoredDueToDisplay": "error",
  "markdown.preview.lineHeight": 1.5,
  "material-icon-theme.activeIconPack": "react_redux",
  "redhat.telemetry.enabled": false,
  "scss.scannerExclude": [
    "**/.git",
    "**/.vscode",
    "**/node_modules",
    "**/bower_components",
    "**/bin",
    "**/build",
    "**/dist",
  ],
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
  },
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.npm": "C:\\Program Files\\nodejs\\npm.cmd",
  "typescript.surveys.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.customMenuBarAltFocus": false,
  "window.enableMenuBarMnemonics": false,
  "window.openFoldersInNewWindow": "on",
  "window.title": "${rootName}${separator}${activeFolderMedium} ${dirty}",
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.enablePreview": false,
  "workbench.list.defaultFindMode": "filter",
  "workbench.list.openMode": "doubleClick",
  "workbench.startupEditor": "none",
  // lint
  "css.lint.importStatement": "error",
  "scss.showErrors": true,
  // formatting
  "files.eol": "\n",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  },
  "[spring-boot-properties-yaml]": {
    "editor.insertSpaces": true,
  },
  "[yaml]": {
    "editor.insertSpaces": true,
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml",
  },
  "editor.formatOnType": false,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
  "eslint.alwaysShowStatus": true,
  "eslint.enable": true,
  "eslint.format.enable": true,
  "eslint.useESLintClass": true,
  "eslint.codeActionsOnSave.rules": [
    "prettier/prettier",
    "linebreak-style",
    "eol-last",
    "no-multiple-empty-lines",
    "@typescript-eslint/lines-between-class-members",
    "semi-style",
    "comma-style",
    "dot-location",
    "operator-linebreak",
    "multiline-ternary",
    "@typescript-eslint/brace-style",
    "react/jsx-curly-newline",
    "object-curly-newline",
    "object-property-newline",
    "array-element-newline",
    "function-paren-newline",
    "function-call-argument-newline",
    "newline-per-chained-call",
    "implicit-arrow-linebreak",
    "indent",
    "no-trailing-spaces",
    "no-multi-spaces",
    "semi-spacing",
    "@typescript-eslint/comma-spacing",
    "space-unary-ops",
    "@typescript-eslint/space-infix-ops",
    "rest-spread-spacing",
    "@typescript-eslint/keyword-spacing",
    "space-in-parens",
    "@typescript-eslint/space-before-function-paren",
    "generator-star-spacing",
    "yield-star-spacing",
    "template-tag-spacing",
    "arrow-spacing",
    "@typescript-eslint/func-call-spacing",
    "@typescript-eslint/space-before-blocks",
    "block-spacing",
    "template-curly-spacing",
    "@typescript-eslint/object-curly-spacing",
    "switch-colon-spacing",
    "key-spacing",
    "computed-property-spacing",
    "no-whitespace-before-property",
    "@typescript-eslint/semi",
    "@typescript-eslint/no-extra-semi",
    "curly",
    "arrow-parens",
    "new-parens",
    "wrap-iife",
    "@typescript-eslint/no-extra-parens",
    "@typescript-eslint/quotes",
    "jsx-quotes",
    "@typescript-eslint/comma-dangle",
    "@typescript-eslint/member-delimiter-style",
    "dot-notation",
    "@typescript-eslint/adjacent-overload-signatures",
    "@typescript-eslint/no-extra-non-null-assertion",
    "@typescript-eslint/no-inferrable-types",
  ],
  "prettier.insertPragma": true,
  "debug.javascript.pickAndAttachOptions": {},
  "window.commandCenter": true,
  "window.zoomLevel": 1,
}
